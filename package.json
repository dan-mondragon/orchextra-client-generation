{
  "name": "orchextra-client-generation-code",
  "version": "1.0.4",
  "description": "Orchextra SDK para el consumo del módulo de Genereación.",
  "main": "campaign.js",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && npm run coverage",
    "coverage": "nyc ava",
    "postinstall": "npm run build",
    "build": "babel src/ --out-dir . --presets es2015 --ignore node_modules/*,test/*,coverage/*,dist/*,.nyc_output/* --no-babelrc  --plugins add-module-exports"
  },
  "ava": {
    "require": "babel-register",
    "files": [
      "test/**/*-test.js"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "test/*",
      "coverage/*"
    ],
    "all": true,
    "reporter": [
      "html",
      "text"
    ]
  },
  "author": "Ricardo Mondragon Leal",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.17.0",
    "bab": "0.0.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "nyc": "^10.0.0",
    "standard": "^8.6.0",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "form-data": "^2.2.0",
    "orchextra-client": "^4.0.0",
    "orchextra-client-generation": "^1.0.4",
    "querystring": "^0.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  }
}
